extends layout

block content
  h1 #{title}
  hr
  ul.list-group
    each article, i in articles
      li.list-group-item
        a(href="/articles/"+article._id)= article.title
  ul.list-group
    each linkup, i in linkups
      li.list-group-item
        a(href="/linkups/"+linkup._id)= linkup.title
  
